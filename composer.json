    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=8.1",
        "pda/pheanstalk": "^4",
        "psr/log": "^1",
        "symfony/console": "^5",
        "symfony/framework-bundle": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9.5"
    },
    "autoload": {
        "psr-4": {
            "Angle\\PheanstalkBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Angle\\PheanstalkBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    }
